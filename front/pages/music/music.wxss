/* pages/music/music.wxss */
.music-list{
  display: flex;
  flex-direction: column;
}
.music-item{
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #7f8c8d;
  padding: 10rpx 40rpx;
  box-sizing: border-box;
  border-top: 1rpx solid rgba(189, 195, 199,0.7);
  overflow: hidden;
}

.play{
  color: #e74c3c;
}